<%- include('../_layouts/adminheader') %>

<h2 class="page-title">Schedule an Interview</h2>
<a href="/admin/interviews" class="btn btn-primary">Back to all interviews</a>
<br><br>

<form method="post" action="/admin/interviews/add-interview" enctype="multipart/form-data">
    
    <div class="form-group">
        <label for="">EmailId</label>
        <input type="text" class="form-control" name="emailid" value="<%= emailid %>" placeholder="EmailId">
    </div>
    
    <div class="form-group">
        <label for="">SkypeId</label>
        <input type="text" class="form-control" name="skypeid" value="<%= skypeid %>" placeholder="SkypeId">
    </div>
    
    
    <div class="form-group">
        <label for="">CellNumber</label>
        <input type="text" class="form-control" name="cellnumber" value="<%= cellnumber %>" placeholder="CellNumber">
    </div>


    
       
    <button class="btn btn-default">Submit</button>
</form>


<%- include('../_layouts/adminfooter') %>